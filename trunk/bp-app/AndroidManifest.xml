<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.ewhoxford.android.bloodpressure" android:versionCode="1"
	android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="true">


		<activity android:name=".MainActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"></action>
				<category android:name="android.intent.category.LAUNCHER"></category>
			</intent-filter>
		</activity>
		<activity android:name=".MeasureActivity"></activity>
		<activity android:name=".MeasureListActivity"></activity>
		<activity android:name=".HelpActivity"></activity>
		<activity android:name=".MeasureViewActivity"></activity>
		<uses-sdk android:minSdkVersion="8" android:targetSdkVersion="11" />

		<provider android:name="com.ewhoxford.android.bloodpressure.database.BPMeasureProvider"
